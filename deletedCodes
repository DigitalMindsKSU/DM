    background-size: cover;
    padding-bottom: 10%;
    margin: 0;
  }
footer {
    position: absolute;
    width: 100%;
    padding-top: 3%;
    padding-bottom: 3%;
  }

  .footer {
    padding: 10px;
    color: #ffffff;
  }

