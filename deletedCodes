  FROM ACHIEVEMENTS

   @media (min-width: 767.0px) and (max-width: 767.9px) {

    .navbar-collapse .fa {
      font-size: 0rem;
    }

    #tabs .nav-link {
      width: 7rem;
    }

    .navbar-collapse .nav-link {
      text-align: center;
      position: relative;
      top: -1rem;
    }
    footer .col-md-7 {
      border-left: none;
    }

  }

  @media (max-width: 767px) {

